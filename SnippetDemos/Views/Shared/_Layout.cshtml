<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/css/jquery-ui")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")

    <script type="text/javascript" src="@Url.ContentVersioned("~/Scripts/Demo/genericAjaxFunctions.js")"></script>
    <link rel="stylesheet" type="text/css" href="@Url.ContentVersioned("~/Content/DataTables/css/jquery.datatables.min.css")">
    <script type="text/javascript" src="@Url.ContentVersioned("~/Scripts/DataTables/jquery.datatables.min.js")"></script>

</head>
<body>
    <div>
        @RenderBody()
        @Scripts.Render("~/bundles/bootstrap")
    </div>
</body>
</html>
